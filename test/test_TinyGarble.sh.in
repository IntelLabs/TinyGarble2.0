	empfile=comp_nbit_ncc_syn.emp.bin
	ref=3
	(${loc}/TinyGarble -n $emploc/$empfile -m 1 -c 8 -r 3 -i 12AB67 "$@" --oo &)>/dev/null 
	output=$(${loc}/TinyGarble -k 2 -n $emploc/$empfile -m 1 -c 8 -r 3 -i D1AB59 "$@" --oo) 
	result

	empfile=full_adder.emp.bin
