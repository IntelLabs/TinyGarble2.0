#!/bin/bash
echo "test_TinyGarble.sh		: test GC back-end in malicious setting"
echo "test_TinyGarble.sh --sh		: test GC back-end in semi-honest setting"
echo "test_TinyGarble.sh --pi		: test programming interface in malicious setting"
echo "test_TinyGarble.sh --pi --sh	: test programming interface in semi-honest setting"

result () {
	o=${output%%	*}
	if [ $o = $ref ]; then
	   echo $empfile ": success!"
	else
	   echo $empfile ": error! expected: " $ref " produced: " $o
	fi
}
loc=@CMAKE_CURRENT_SOURCE_DIR@/bin
emploc=${loc}/netlists

if [ ! -f "$emploc/comp_nbit_ncc_syn.emp.bin" ]; then
    ${loc}/unpack.sh
fi

if [ $1 ] && [ $1 = "--pi" ]; then
	${loc}/TinyGarblePI "$@" < test/rand.txt > /dev/null & ${loc}/TinyGarblePI "$@" -k 2 < test/rand.txt
